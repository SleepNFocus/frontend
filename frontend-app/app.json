{
  "expo": {
    "name": "focuz-app",
    "slug": "focuz-app",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/_redirects"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.focuz-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "build": {
        "babel": {
          "include": [
            "node_modules/expo"
          ]
        }
      }
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "focuz-app",
    "experiments": {
      "typedRoutes": true
    }
  }
}
